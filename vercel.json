{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
        "type": "module",
        "scripts": {
          "start": "node index.js",
          "build": "echo 'No build step needed'"
        }
  }
  